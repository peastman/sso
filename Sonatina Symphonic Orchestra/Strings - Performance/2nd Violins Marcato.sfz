// -------
// Sustain
// -------

<group>
ampeg_attack=0.175
ampeg_vel2attack=-0.175
ampeg_release=1
amp_veltrack=0
volume=-30
gain_cc1=30

#include "includes/2nd-violins-sustain.sfz"


